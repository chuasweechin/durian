<div class="container mb-4 compare">
	<h2>Price Compare with Other Durian Store</h2>

	<h5><%= link_to @url, @url %></h5>

	<table class="table table-striped">
	    <thead>
	        <tr>
	            <th scope="col"></th>
	            <th scope="col">Item</th>
	            <th scope="col">Price per kg</th>
	        </tr>
	    </thead>
	    <tbody>
	    		<% @products.each_with_index do |product, index| %>
	      		<tr>
	              <td>
	              	<%= index + 1 %>
	              </td>

	              <td>
	              	<%= product["name"] %>
	              </td>

	              <td>
	              	<%= "$" + product["price"] %>
	              </td>
	          </tr>
	    		<% end %>
	    </tbody>
	</table>
</div>